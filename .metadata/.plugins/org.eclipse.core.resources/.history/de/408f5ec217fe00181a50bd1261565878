package animalProject;

public class Animal {

	private String name;
	
	//Constructors
	public Animal() {
		super();
	}


	public Animal(String name) {
		super();
		this.name = name;
	}
	
	
	//Getters and setters
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
	
}
